@model List<HRAgencySystem.Web.ViewModels.User.UserReservationsViewModel>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@if (@Model.Any())
{
    <table id="my-reservations" class="table table-striped table-hover table-user-reservations">
        <thead>
            <tr>
                <th class="hidden-phone">Hall name</th>
                <th>Description</th>
                <th>Capacity</th>
                <th class="hidden-phone">Start date</th>
                <th class="hidden-phone">End date</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @Html.DisplayFor(x => x)
        </tbody>
    </table>
}
else
{
    <p>No reservations!</p>
}

